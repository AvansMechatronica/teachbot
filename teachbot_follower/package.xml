<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>teachbot_follower</name>
  <version>0.0.0</version>
  <description>TeachBot Follower - Commands robot using MoveIt based on teachbot input</description>
  <maintainer email="GerardAnneHarkema@gmail.com">gerard</maintainer>
  <license>TODO: License declaration</license>

  <depend>rclpy</depend>
  <depend>sensor_msgs</depend>
